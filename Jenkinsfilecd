pipeline {
    agent any 
    stages {
        stage('Push to docker hub') {
            steps {
                script {
                    withCredentials([string(credentialsId: '', variable: 'dockerhub_pwd')]) {
                        sh "docker login -u sreejitheyne -p ${dockerhub_pwd}"
}
                        sh 'docker push sreejitheyne/javaqr:latest'
                }
            }
        }

    }

 }
