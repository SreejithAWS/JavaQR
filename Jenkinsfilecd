pipeline {
    agent any 
    stages {
        stage('Push to docker hub') {
            steps {
                script {
                   withCredentials([string(credentialsId: 'dockerhub', variable: 'dockerhubpasswd')]){
                        sh "docker login -u sreejitheyne -p ${dockerhubpasswd}"
                    }
                        sh 'docker push sreejitheyne/openjdk:17.0.2'
                }
            }
        }

    }

 }
